{
  "name": "umbraco-umbrella",
  "version": "2.0.1",
  "description": "Umbraco Umbrella",
  "author": "CTI Digital",
  "license": "ISC",
  "scripts": {
    "storybook": "start-storybook -p 6006 -c .storybook -s ./src",
    "build-storybook": "build-storybook -c .storybook -s ./src",
    "start": "npm run dev && npm run watch",
    "watch": "webpack --watch --config .build/webpack.dev.js --mode=development",
    "server": "webpack-dev-server --config .build/webpack.dev.js --mode=development",
    "dev": "npm run clean && npm run build:icons && npm run build:copy && npm run watch:css && webpack --config .build/webpack.dev.js --mode=development",
    "build": "npm run clean && npm run build:icons && npm run build:copy && npm run watch:css && npm run build:assets",
    "watch:css": "postcss src/assets/css/tailwind/index.css -o src/assets/css/tailwind/tailwind.css",
    "build:assets": "webpack --config .build/webpack.prod.js --mode=production",
    "build:images": "node .build/tasks/images",
    "build:icons": "node .build/tasks/icons",
    "build:copy": "node .build/tasks/copy",
    "clean": "node .build/tasks/clean"
  },
  "dependencies": {
    "@babel/preset-react": "^7.12.13",
