    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "lazysizes": "^5.3.0",
    "luxon": "^1.26.0",
    "normalize.css": "^8.0.1",
    "objectFitPolyfill": "^2.3.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-cli": "^8.3.1",
